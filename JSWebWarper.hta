<script language="javascript">
function installServer() {
var objShell = new ActiveXObject("WScript.shell");
objShell.run("npm install --local http-server");
}
function startServer() {
var objShell = new ActiveXObject("WScript.shell");
objShell.run("http-server");
}
function reloadApp() {
location.reload();
}
function killServer() {
var objShell = new ActiveXObject("WScript.shell");
objShell.run("taskkill /f /im node.exe");
}
</script>
<BODY>
<H2>JS Web Warper</H2>
<br>
<H4>Html Aplication by : Maxxx#5509 
<br>
		<input type="button" style="height:50px;width:150px" value="Intsall LocalServer" onclick="installServer(this.value)"></input>   
		<input type="button" style="height:50px;width:150px" value="Start Local Server" onclick="startServer(this.value)"></input>
		<input type="button" style="height:50px;width:150px" value="Stop Local Server" onclick="killServer(this.value)"></input>
		<input type="button" style="height:50px;width:150px" value="Refresh" onclick="reloadApp(this.value)"></input>
